{
  "name": "preact-particles-demo",
  "private": true,
  "version": "1.3.0-alpha.14",
  "description": "> TODO: description",
  "author": "Matteo Bruni <ar3s@icloud.com>",
  "homepage": "https://github.com/matteobruni/tsparticles#readme",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteobruni/tsparticles.git"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "scripts": {
    "build": "npx copyfiles -a ../../node_modules/preact/* ./node_modules/preact/ && npx copyfiles -a ../../node_modules/preact/**/* ./node_modules/preact/ && npx copyfiles -a ../../node_modules/preact-render-to-string/* ./node_modules/preact-render-to-string/ && npx copyfiles -a ../../node_modules/preact-render-to-string/**/* ./node_modules/preact-render-to-string/ && npx preact build",
    "start": "sirv build --cors --single",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "bugs": {
    "url": "https://github.com/matteobruni/tsparticles/issues"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "copyfiles": "^2.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.3",
    "eslint": "^7.9.0",
    "eslint-config-preact": "^1.1.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "identity-obj-proxy": "^3.0.0",
    "preact-cli": "^3.0.2",
    "preact-render-spy": "^1.3.0",
    "prettier": "^2.1.2",
    "sirv-cli": "^1.0.6"
  },
  "dependencies": {
    "preact": "^10.4.8",
    "preact-particles": "^1.18.0-alpha.14",
    "preact-render-to-string": "^5.1.10",
    "preact-router": "^3.2.1"
  }
}
