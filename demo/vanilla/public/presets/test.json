{
  "background": {
    "color": "#000"
  },
  "fpsLimit": 60,
  "interactivity": {
    "detectsOn": "canvas",
    "events": {
      "resize": true
    }
  },
  "particles": {
    "color": {
      "value": "#ff0000",
      "animation": {
        "enable": true,
        "speed": 5,
        "sync": true
      }
    },
    "move": {
      "noise": {
        "enable": true
      },
      "direction": "none",
      "enable": true,
      "outModes": {
        "default": "out"
      },
      "random": false,
      "speed": 1,
      "straight": false,
      "trail": {
        "enable": true,
        "length": 1000
      },
      "warp": true
    },
    "number": {
      "value": 0
    },
    "opacity": {
      "value": 0.3
    },
    "shape": {
      "type": "circle"
    },
    "size": {
      "value": 0.5
    }
  },
  "detectRetina": true,
  "emitters": {
    "size": {
      "width": 0,
      "height": 0
    },
    "life": {
      "count": 20,
      "duration": 1
    },
    "rate": {
      "delay": 0.1,
      "quantity": 5
    }
  }
}
