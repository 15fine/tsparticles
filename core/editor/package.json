    "@types/dat.gui": "^0.7.5",
    "babel-loader": "^8.1.0",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^3.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
