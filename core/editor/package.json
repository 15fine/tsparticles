{
  "name": "tsparticles-editor",
  "version": "1.19.0-alpha.0",
  "description": "tsParticles Configuration Editor",
  "keywords": [
    "tsparticles",
    "particles.js",
    "particles",
    "editor",
    "dat.gui",
    "javascript",
    "wysiwyg"
  ],
  "author": "Matteo Bruni <ar3s@icloud.com>",
  "homepage": "https://particles.matteobruni.it/",
  "license": "MIT",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/cjs/index.js",
  "jsdelivr": "dist/tsparticles.editor.min.js",
  "unpkg": "dist/tsparticles.editor.min.js",
  "browser": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteobruni/tsparticles.git",
    "directory": "core/editor"
  },
  "scripts": {
    "prettify:src": "prettier --write ./src/",
    "prettify:readme": "prettier --write ./README.md",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "clear:dist": "rimraf -f ./dist",
    "build:ts:esm": "tsc -b tsconfig.esm.json",
    "build:ts:cjs": "tsc",
    "build:ts": "yarn build:ts:browser && yarn build:ts:esm && yarn build:ts:cjs",
    "build:ts:check": "tsc --noEmit",
    "build:ts:browser": "tsc -b tsconfig.browser.json",
    "build": "yarn clear:dist && yarn prettify:src && yarn build:ts:check && yarn lint && yarn build:ts && webpack --mode production && yarn prettify:readme",
    "prepack": "yarn build"
  },
  "bugs": {
    "url": "https://github.com/matteobruni/tsparticles/issues"
  },
  "dependencies": {
    "object-gui": "^1.0.11",
    "tsparticles": "^1.19.0-alpha.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-loader": "^8.2.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "fibers": "^5.0.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.5",
    "terser-webpack-plugin": "^5.0.3",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.2.0"
  }
}
