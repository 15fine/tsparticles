  "homepage": "https://particles.matteobruni.it/",
  "license": "ISC",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/tsparticles.editor.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteobruni/tsparticles.git"
